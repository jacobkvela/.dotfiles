# Ignore generated/cache files
*.swp
*.swo
*~
.DS_Store

# Ignore history files
.bash_history
.viminfo
.python_history

# Tmux plugins (managed by TPM)
tmux/.config/tmux/plugins/
tmux/.tmux/plugins/

# Fish generated files
fish/.config/fish/fish_variables
fish/.config/fish/fishd.*

# MPD generated files
mpd/.config/mpd/database
mpd/.config/mpd/state
mpd/.config/mpd/pid
mpd/.config/mpd/sticker.sql
mpd/.config/mpd/*.log
mpd/.config/mpd/log

# Vim plugins (managed by plugin manager)
vim/.vim/plugged/
vim/.vim/bundle/
vim/.vim/pack/

# Neovim plugins
nvim/.config/nvim/plugin/
nvim/.local/share/nvim/

# GitHub CLI tokens (IMPORTANT!)
gh/.config/gh/hosts.yml

# wildcard for all error logs
**/*error.log
**/*.log
*.log
